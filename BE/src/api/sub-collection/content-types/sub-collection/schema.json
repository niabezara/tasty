{
  "kind": "collectionType",
  "collectionName": "sub_collections",
  "info": {
    "singularName": "sub-collection",
    "pluralName": "sub-collections",
    "displayName": "SubCollection"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "sub_collections"
    },
    "subTitle": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "recipe": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::recipe.recipe",
      "inversedBy": "sub_collections"
    }
  }
}
